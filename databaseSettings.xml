<application>
  <component name="DatabaseSettings" version="2">
    <text-mode in-scripts="true" in-literals="true">
      <parameter-patterns>
        <parameter-pattern value="\?\d*" />
        <parameter-pattern value="\:\w+" />
        <parameter-pattern value="\@\w+" />
        <parameter-pattern value="\%\w+" />
        <parameter-pattern value="\$\{[^$\{\}]*\}" />
        <parameter-pattern value="\$(\w+)\$" scope="*,-PostgreSQL" predefined="true" />
        <parameter-pattern value="\#\w+\#" />
        <parameter-pattern value="\?(\d+)" in-scripts="false" in-literals="false" />
        <parameter-pattern value=":(\w+)" in-scripts="false" in-literals="false" scope="*,-PostgreSQL" predefined="true" />
        <parameter-pattern value="%\w+" in-scripts="false" in-literals="false" scope="JAVA,Python" predefined="true" />
        <parameter-pattern value="\$\{([^\{\}]*)\}" in-scripts="false" in-literals="false" />
        <parameter-pattern value="\$\(([^\)]+)\)" in-scripts="false" in-literals="false" />
        <parameter-pattern value="#(\w+)#" in-scripts="false" in-literals="false" scope="XML" predefined="true" />
        <parameter-pattern value="(?&lt;=\W|\A)\$(\d+|[a-zA-Z_](?:\w|\.)*)\$?(?=\W|\z)" in-scripts="false" in-literals="false" scope="*,-SQL,Oracle,OracleSqlPlus,PostgreSQL,PostgresPLSQL" predefined="true" />
        <parameter-pattern value="(?&lt;=\W|\A)#(\d+|[a-zA-Z_](?:\w|\.)*)#?(?=\W|\z)" in-scripts="false" in-literals="false" scope="*,-SQL,Oracle,OracleSqlPlus,PostgreSQL,PostgresPLSQL" predefined="true" />
        <parameter-pattern value="%\((\w+)\)s" in-scripts="false" in-literals="false" scope="Python" predefined="true" />
      </parameter-patterns>
    </text-mode>
    <csv-formats>
      <csv-format name="Tab-separated (TSV)" id="Tab-separated (TSV)_id">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="&#9;" recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
        <header>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="&#9;" recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </header>
      </csv-format>
      <csv-format name="Comma-separated (CSV)" id="Comma-separated (CSV)_id">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="," recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
      </csv-format>
    </csv-formats>
  </component>
</application>